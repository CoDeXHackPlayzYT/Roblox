-- Scripts:
function SCRIPT_MEDM71_FAKESCRIPT() -- Close.LocalScript 
local script = Instance.new('LocalScript')
script.Parent = Close
script.Parent.MouseButton1Click:Connect(function()
game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame.Visible = false
end)


end
coroutine.resume(coroutine.create(SCRIPT_MEDM71_FAKESCRIPT))
function SCRIPT_TOZW80_FAKESCRIPT() -- Main.LocalScript 
local script = Instance.new('LocalScript')
script.Parent = Main
function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

counter = 0

while wait(0.1)do
script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

counter = counter + 0.01
end




end
coroutine.resume(coroutine.create(SCRIPT_TOZW80_FAKESCRIPT))
function SCRIPT_BGEC88_FAKESCRIPT() -- Open.LocalScript 
local script = Instance.new('LocalScript')
script.Parent = Open
script.Parent.MouseButton1Click:Connect(function()
game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame.Visible = true
end)

end
coroutine.resume(coroutine.create(SCRIPT_BGEC88_FAKESCRIPT))
function SCRIPT_ZOLF79_FAKESCRIPT() -- Open.LocalScript 
local script = Instance.new('LocalScript')
script.Parent = Open
function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

counter = 0

while wait(0.1)do
script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

counter = counter + 0.01
end




end
coroutine.resume(coroutine.create(SCRIPT_ZOLF79_FAKESCRIPT))
    
end


